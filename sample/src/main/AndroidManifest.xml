<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="io.git.zjoker.sample">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <application
        android:name=".MyApp"
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="io.git.zjoker.sample.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".StringCacheActivity" />
        <activity android:name=".BitmapCacheActivity" />
        <activity android:name=".SerializableCacheActivity" />
        <activity android:name=".CacheLevelActivity" />
        <activity android:name=".MultiplyCachePathActivity" />
        <activity android:name=".CustomConverterActivity" />
        <activity android:name=".ObjCacheActivity" />
    </application>

</manifest>